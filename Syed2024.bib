	number = {1},
	pages = {1--12},
	title = {Self-consistent 3J coupling analysis for the joint calibration of Karplus coefficients and evaluation of torsion angles},
	url = {https://doi.org/10.1023/A:1008345303942},
	volume = {14},
	year = {1999}}

