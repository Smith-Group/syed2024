	number = {2},
	pages = {248--282},
	title = {SPINEVOLUTION: A powerful tool for the simulation of solid and liquid state NMR experiments},
	url = {https://www.sciencedirect.com/science/article/pii/S1090780705002442},
	volume = {178},
